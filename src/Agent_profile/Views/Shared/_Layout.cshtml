<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Agent profile</title>

    <environment names="Development">
        <link href="~/css/normalize.css" rel="stylesheet" />
        <link href="~/css/jquery.formstyler.css" rel="stylesheet" />
        <link href="~/css/jquery.formstyler.theme.css" rel="stylesheet" />
        <link href="~/css/slick.css" rel="stylesheet" />
        <link href="~/css/slick-theme.css" rel="stylesheet" />
        <link href="~/css/style.css" rel="stylesheet" />
        <link rel="stylesheet" type="text/css" href="https://cloud.typography.com/6744776/6682792/css/fonts.css">
    </environment>
    <environment names="Staging,Production">
        @*<link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/css/bootstrap.min.css"
                  asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
                  asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
            <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />*@
    </environment>
</head>
<body>
    <!--==============================Header=================================-->
    <header id="header" class="c-header">
        <div class="l-row l-row--logo h-clearfix">
            <div class="c-header__logo">
                <a href="index.html">
                    <span class="c-header__logo-icon"></span>
                    <span class="c-header__logo-txt">Houses.Forsale</span>
                </a>
            </div>
            <div class="c-header__login">
                <div class="c-header__login-toggle h-clearfix">
                    <div class="c-header__login-icon">
                        <div class="c-header__login-avatar">
                            <img src="~/images/agents/agent1.png" alt="">
                            <span class="c-header__user-notices c-header__user--has-notices">1</span>
                        </div>
                    </div>
                    <div class="c-header__login-btn">
                        <span class="c-header__login-txt">Login</span>
                        <span class="c-header__login-carrot"></span>
                        <span class="c-header__login-close l-close-btn"></span>
                    </div>
                </div>
                <div class="l-dropdown l-dropdown--login">
                    <form class="l-form l-login-form">
                        <div class="l-form__holder">
                            <div class="l-form__inner">
                                <div class="l-form-row">
                                    <div class="l-form-group">
                                        <input type="email" class="l-form-control" id="inputEmailDropdown" required>
                                        <label for="inputEmailDropdown" class="l-form-label">email</label>
                                        <span class="l-form-control-focus"></span>
                                    </div>
                                </div>
                                <div class="l-form-row">
                                    <div class="l-form-group">
                                        <input type="password" class="l-form-control" id="inputPasswordDropdown" required>
                                        <label for="inputPasswordDropdown" class="l-form-label">password</label>
                                        <span class="l-form-control-focus"></span>
                                        <a href="#" class="l-form-link">Forgot Password?</a>
                                    </div>
                                </div>
                                <div class="l-form-row l-form-row--submit l-row--center">
                                    <button type="submit" class="l-btn l-btn-primary">Sign in</button>
                                    <div class="l-form-text">
                                        <a href="#" class="l-form-link">Create an Account</a>
                                    </div>
                                </div>
                                <div class="l-form-row l-form-row--login-social">
                                    <div class="l-divider l-divider--form l-divider--light"><span>or</span></div>
                                    <div class="l-form-buttons">
                                        <button type="button" class="l-btn l-btn-login l-btn-login--facebook">Sign in with Facebook</button>
                                        <button type="button" class="l-btn l-btn-login l-btn-login--google">Sign in with Google</button>
                                        <button type="button" class="l-btn l-btn-login l-btn-login--microsoft">Sign in with Microsoft</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <ul class="l-dropdown l-dropdown--user-menu">
                    <li class="notices">
                        <a href="#"><span class="c-header__user-notices c-header__user--has-notices">1</span> Notices</a>
                    </li>
                    <li>
                        <a href="#">Account Settings</a>
                    </li>
                    <li>
                        <a href="#">Saved Searches</a>
                    </li>
                    <li>
                        <a href="#">Viewed Homes</a>
                    </li>
                    <li>
                        <a href="#">Favorited Homes</a>
                    </li>
                    <li>
                        <a href="#">Log Out</a>
                    </li>
                </ul>
            </div>
            <div class="c-header__nav">
                <ul class="h-clearfix">
                    <li class="has-dropdown">
                        <span>Buy</span>
                        <ul class="l-dropdown l-dropdown--left">
                            <li class="user-location">
                                84004 - Alpine
                            </li>
                            <li>
                                <a href="#">All Homes</a>
                            </li>
                            <li>
                                <a href="#">New Construction</a>
                            </li>
                            <li>
                                <a href="#">Forclosures</a>
                            </li>
                            <li>
                                <a href="#">Land</a>
                            </li>
                            <li>
                                <a href="#">Recently Sold</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="#">Sell</a>
                    </li>
                    <li>
                        <a href="#">Neighborhood</a>
                    </li>
                    <li>
                        <a href="#">Mortgage</a>
                    </li>
                    <li class="has-dropdown">
                        <span>Agents</span>
                        <ul class="l-dropdown l-dropdown--right">
                            <li>
                                <a href="#">Looking for an Agent</a>
                            </li>
                            <li>
                                <a href="#">Advertising Solutions</a>
                            </li>
                            <li>
                                <a href="#">Create Agent Profile</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </header>
    <button type="button" class="c-header__nav-toggle">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="c-header__nav-close l-close-btn"></span>
    </button>
    <div id="content" class="c-content">
        @RenderBody()
    </div>

    <!--==============================Footer=================================-->
    <footer id="footer" class="c-footer">
        <div class="c-footer__equal-housing-logo"></div>
        <div class="l-row l-row--footer-logo">
            <div class="l-container">
                <div class="c-footer__logo">
                    <a href="index.html">Homes.Forsale</a>
                </div>
            </div>
        </div>
        <div class="l-row l-row--copyright">
            <div class="l-container">
                <ul class="l-list l-list--social c-footer__social">
                    <li>
                        <a href="#" target="_blank">
                            <span class="l-list--social__icon">

                                <img src="~/images/icon-instagram.svg" />
                            </span>
                            <span class="l-list--social__icon--hover">

                                <img src="~/images/icon-instagram-white.svg" />
                            </span>
                        </a>
                    </li>
                    <li>
                        <a href="#" target="_blank">
                            <span class="l-list--social__icon">
                                <img src="~/images/icon-twitter.svg" />
                            </span>
                            <span class="l-list--social__icon--hover">
                                <img src="~/images/icon-twitter-white.svg" />
                            </span>
                        </a>
                    </li>
                    <li>
                        <a href="#" target="_blank">
                            <span class="l-list--social__icon">
                                <img src="~/images/icon-facebook.svg" />
                            </span>
                            <span class="l-list--social__icon--hover">
                                <img src="~/images/icon-facebook-white.svg" />
                            </span>
                        </a>
                    </li>
                    <li>
                        <a href="#" target="_blank">
                            <span class="l-list--social__icon">
                                <img src="~/images/icon-linkedin.svg" />
                            </span>
                            <span class="l-list--social__icon--hover">
                                <img src="~/images/icon-linkedin-white.svg" />
                            </span>
                        </a>
                    </li>
                    <li>
                        <a href="#" target="_blank">
                            <span class="l-list--social__icon">
                                <img src="~/images/icon-youtube.svg" />
                            </span>
                            <span class="l-list--social__icon--hover">
                                <img src="~/images/icon-youtube-white.svg" />
                            </span>
                        </a>
                    </li>
                </ul>
                <div class="c-footer__copyright">
                    <p>
                        &copy; Copyright 2017, Real Estate Guides. All rights are reserved.
                    </p>
                    <p>
                        Updated January 2016: By searching, you agree to the <a href="#">Terms of Use</a>, <a href="#">Privacy Policy</a> and <a href="#">End-User License Agreement.</a>
                    </p>
                </div>
            </div>
        </div>
    </footer>
    <!--======================== Sticky Footer ======================================-->
    <footer id="footer-mobile" class="c-footer c-footer--mobile c-footer--mobile-agent">
        <div class="c-footer__links c-footer__links--agent">
            <a class="l-link l-link--action l-link--call" href="#"><span>Call</span></a>
            <a class="l-link l-link--action l-link--contact" href="mailto:email@goes.here"><span>Contact</span></a>
            <a class="l-link l-link--action l-link--review" href="#"><span>Leave Review</span></a>
        </div>
        <div class="c-footer__links c-footer__links--property">
            <a class="l-link l-link--action l-link--save" href="#"><span>Save</span></a>
            <a class="l-link l-link--action l-link--agent" href="#"><span>Agent</span></a>
            <a class="l-link l-link--action l-link--direction" href="#"><span>Directions</span></a>
            <a class="l-link l-link--action l-link--share" href="#"><span>Share</span></a>
        </div>
    </footer>
    <!--======================== Mobile Navigation ==================================-->
    <div class="c-mobile-wrap">
        <div class="c-mobile-wrap__logo">
            <a href="index.html">
                <span class="c-mobile-wrap__logo-icon"></span>
            </a>
        </div>
        <div class="c-mobile-wrap__nav">
            <ul>
                <li class="has-dropdown form-toggle">
                    <span class="has-dropdown__title">My account</span>
                    <ul class="l-dropdown--mobile">
                        <li class="notices">
                            <a href="#"><span class="c-header__user-notices c-header__user--has-notices">1</span> Notices</a>
                        </li>
                        <li>
                            <a href="#">Account Settings</a>
                        </li>
                        <li>
                            <a href="#">Saved Searches</a>
                        </li>
                        <li>
                            <a href="#">Viewed Homes</a>
                        </li>
                        <li>
                            <a href="#">Favorited Homes</a>
                        </li>
                        <li>
                            <a href="#">Log Out</a>
                        </li>
                    </ul>
                </li>
                <li class="has-dropdown">
                    <span class="has-dropdown__title">Buy</span>
                    <ul class="l-dropdown--mobile">
                        <li>
                            <a href="#">All Homes</a>
                        </li>
                        <li>
                            <a href="#">New Construction</a>
                        </li>
                        <li>
                            <a href="#">Forclosures</a>
                        </li>
                        <li>
                            <a href="#">Land</a>
                        </li>
                        <li>
                            <a href="#">Recently Sold</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a href="#">Sell</a>
                </li>
                <li>
                    <a href="#">Neighborhood</a>
                </li>
                <li>
                    <a href="#">Mortgage</a>
                </li>
                <li class="has-dropdown">
                    <span class="has-dropdown__title">Agents</span>
                    <ul class="l-dropdown--mobile">
                        <li>
                            <a href="#">Looking for an Agent</a>
                        </li>
                        <li>
                            <a href="#">Advertising Solutions</a>
                        </li>
                        <li>
                            <a href="#">Create Agent Profile</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
    <!--======================== Mobile Forms ================================-->
    <div class="c-mobile-form-wrap">
        <span class="c-mobile-form__close l-close-btn"></span>
        <div class="c-mobile-form-wrap__inner">
            <form class="l-form l-login-form l-login-form--mobile">
                <div class="l-form__holder">
                    <h3>Sign in</h3>
                    <div class="l-form__inner">
                        <div class="l-form-row">
                            <div class="l-form-group">
                                <input type="email" class="l-form-control" id="inputEmail_SignInFormMobile" required>
                                <label for="inputEmail_SignInFormMobile" class="l-form-label">email</label>
                                <span class="l-form-control-focus"></span>
                            </div>
                        </div>
                        <div class="l-form-row">
                            <div class="l-form-group">
                                <input type="password" class="l-form-control" id="inputPassword_SignInFormMobile" required>
                                <label for="inputPassword_SignInFormMobile" class="l-form-label">password</label>
                                <span class="l-form-control-focus"></span>
                                <a href="#" class="l-form-link">Forgot Password?</a>
                            </div>
                        </div>
                        <div class="l-form-row l-form-row--submit l-row--center">
                            <button type="submit" class="l-btn l-btn-primary">Sign in</button>
                            <div class="l-form-text">
                                <a href="#" class="l-form-link">Create an Account</a>
                            </div>
                        </div>
                        <div class="l-form-row l-form-row--login-social">
                            <div class="l-divider l-divider--form l-divider--light"><span>or</span></div>
                            <div class="l-form-buttons">
                                <button type="button" class="l-btn l-btn-login l-btn-login--facebook">Sign in with Facebook</button>
                                <button type="button" class="l-btn l-btn-login l-btn-login--google">Sign in with Google</button>
                                <button type="button" class="l-btn l-btn-login l-btn-login--microsoft">Sign in with Microsoft</button>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>

    <environment names="Development">
        @*<script src="~/lib/jquery/dist/jquery.js"></script>*@
        @*<script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>*@
        <script src="~/js/jquery-1.12.3.min.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/waypoints/2.0.3/waypoints.min.js"></script>
        <script src="~/js/jquery.formstyler.min.js"></script>
        <script src="~/js/slick.min.js"></script>
        <script src="~/js/Chart.bundle.min.js"></script>
        <script src="~/js/custom.js"></script>
        <script src="~/js/ie10-viewport-bug-workaround.js"></script>


    </environment>
    <environment names="Staging,Production">
        @*<script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                    asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                    asp-fallback-test="window.jQuery">
            </script>
            <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.6/bootstrap.min.js"
                    asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                    asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
            </script>
            <script src="~/js/site.min.js" asp-append-version="true"></script>*@
    </environment>

    @RenderSection("scripts", required: false)
</body>
</html>
